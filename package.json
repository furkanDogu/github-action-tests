{
  "name": "github-actions-node",
  "version": "1.0.0",
  "description": "An experiment project for github actions",
  "main": "index.js",
  "engines": {
    "node": "v14.15.0"
  },
  "scripts": {
    "test": "jest",
    "dev": "nodemon --exec babel-node ./index.js",
    "start": "npm run build && node ./build/index.js",
    "build": "npm run clean && npm run build-babel",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./index.js -s",
    "db:migration:make": "knex migrate:make",
    "db:migration:up": "knex migrate:latest",
    "db:migration:down": "knex migrate:rollback"
  },
  "author": "Furkan Dogu",
  "license": "ISC",
  "devDependencies": {
    "@babel/node": "^7.13.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.1",
    "@babel/runtime": "^7.14.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "dotenv": "^9.0.0",
    "knex": "^0.95.4",
    "pg": "^8.6.0"
  }
}
